{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "src/index.js",
  "scripts": {
    "prestart": "npm run migrations",
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node  src/index.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test nyc mocha -r regenerator-runtime -r @babel/register --timeout 80000 --exit ./test/**/*.js",
    "pretest": "cross-env NODE_ENV=test npm run drop-db && cross-env NODE_ENV=test npm run create-db && cross-env NODE_ENV=test npm run migrations && cross-env NODE_ENV=test npm run seed",
    "coverage": "nyc --reporter=lcov npm test",
    "admin:create": "babel-node src/database/superAdmin.js",
    "role:create": "babel-node src/database/superAdminRole.js"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@sendgrid/mail": "^7.4.0",
    "@types/regenerator-runtime": "^0.13.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "datauri": "^3.0.0",
    "dotenv": "^6.2.0",
    "ejs": "^2.7.4",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "i18n": "^0.13.2",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-simple": "^0.5.6",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemon": "^2.0.6",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.5.1",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.87.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.0.1",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.4",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0"
  }
}
